<script>

function toggleAutoScored()
{
  var chToggle = document.getElementById("row_toggle");

  if (chToggle.checked) {
      cName = "hide_element";
  } 
  else {
      cName = "show_element";
  }

  document.getElementById("item_row_scored").className = cName;
  document.getElementById("score_row_scored").className = cName;

}

function showResponse()
{
  document.getElementById("blank_row").className = "show_element";
  document.getElementById("response_detail").className = "show_element";

}
</script>

